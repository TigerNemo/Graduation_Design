           interval: 200,\n              min: 0,\n              max: 1400+200,\n              splitNumber: 7,\n              axisLine: {\n                symbol: ['none', 'arrow'],\n                symbolSize: [6, 6],\n                lineStyle: {\n                  color: '#122C49'\n                }\n              },\n              axisLabel: {\n                color: '#61B9C8',\n                showMaxLabel: false,\n                fontSize: 10\n              },\n              splitLine: {\n                show: false,\n              },\n              name: '(个)',\n              nameGap: -10,\n              nameTextStyle: {\n                color: '#61B9C8',\n                fontSize: 9,\n                align: 'right',\n                padding: [0, 6, 0, 0]\n              },\n            },\n        series: [{\n          name: '核销个数',\n          type: 'line',\n          symbol: 'none',\n          smooth: true,\n          data: [],\n          lineStyle: {\n            width: 0\n          },\n          itemStyle: {\n            color: '#9e70ff'\n          },\n          areaStyle: {\n            color: {\n              type: 'linear',\n              x: 0,\n              y: 0,\n              x2: 0,\n              y2: 1,\n              colorStops: [{\n                offset: 0, color: '#9e70ff' // 0% 处的颜色\n              },\n                {\n                  offset: 0.4, color: '#9e70ff' // 40% 处的颜色\n                }, {\n                  offset: 1, color: '#6e5eff' // 100% 处的颜色\n                }],\n              global: false // 缺省为 false\n            }, //背景渐变色\n            origin: 'start'\n          }\n        },\n        ]\n      }\n    }\n  },\n  methods: {\n    setChart() {\n      let myChart = this.$echarts(this.$el);\n      myChart.clear();\n      myChart.resize()\n      myChart.setOption(this.option);\n    },\n    load() {\n      request.get(\"/EverydayDate\").then(res => {\n        const { data } = res;\n        this.option.xAxis.data = data[0]\n        this.option.series[0].data = data[1]\n        this.setChart()\n      })\n    }\n  },\n  mounted() {\n    this.load()\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.barChart {\n    width: 100%;\n    height: 100%;\n}\n</style>"],"sourceRoot":"src/views/components/page"}]}