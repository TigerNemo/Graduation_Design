rList = [\n                                        'rgb(44,123,254)', 'rgb(194,35,42)', 'rgb(162,98,242)', 'rgb(254,103,44)', 'rgb(44,252,254)'\n                                    ];\n                                    return colorList[params.dataIndex];\n                                }\n                            },\n                        },\n                        data: [\n                            {value: 80, name: '陌生手机'},\n                            {value: 50, name: '破解攻击'},\n                            {value: 40, name: '离线断网'},\n                            {value: 20, name: '异常操作'},\n                            {value: 5, name: '策略违规'}\n                        ]\n                    }\n                ]\n            };\n\n            let myChart = this.$echarts(this.$el);\n\n            myChart.clear();\n            myChart.resize()\n            myChart.setOption(option);\n        },\n    },\n    mounted() {\n        this.setChart()\n    },\n}\n</script>\n\n<style lang=\"less\" scoped>\n.funnel {\n    width: 100%;\n    height: 100%;\n}\n</style>"],"sourceRoot":"src/views/components/page1"}]}