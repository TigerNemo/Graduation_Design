nts/page","sourcesContent":["<template>\n    <div class=\"DistanceConsume\"></div>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: '',\n  data() {\n    return {\n      option: {\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: {            // 坐标轴指示器，坐标轴触发有效\n            type: 'shadow'        // 默认为直线，可选为：'line' | 'shadow'\n          },\n          backgroundColor: '#11367a',\n          textStyle: {\n            color: '#6dd0e3',\n            fontSize: 10,\n          },\n        },\n        legend: {\n          right:\"5%\",\n          top: \"12%\",\n          itemWidth: 7,\n          itemHeight: 7,\n          textStyle: {\n            color: '#5CB1C1',\n          }\n        },\n        grid: {\n          top: '12%',\n          bottom: '12%',\n          left: \"18%\",\n          right: '10%',\n          containLabel: false\n        },\n        xAxis: [\n          {\n            type: 'category',\n            boundaryGap: true,\n            axisLine: {\n              symbol: ['none', 'arrow'],\n              symbolSize: [6, 6],\n              symbolOffset: [0, 10],\n              lineStyle: {\n                color: '#122C49'\n              }\n            },\n            name: '(公里)',\n            nameGap: 5,\n            nameTextStyle: {\n              color: '#61B9C8',\n              fontSize: 10,\n              align: 'top',\n              padding: [26, 0, 0, -3]\n            },\n            axisTick: {show: false},\n            axisLabel: {\n              color: '#61B9C8',\n              fontSize: 10,\n              interval: 0,\n            },\n            data: []\n          }\n        ],\n        yAxis: [\n          {\n            type: 'value',\n            scale: true,\n            max: 1000000 + 200000,\n            min: 0,\n            interval: 200000,\n            axisLine: {\n              symbol: ['none', 'arrow'],\n              symbolSize: [6, 6],\n              lineStyle: {\n                color: '#122C49'\n              }\n            },\n            axisLabel: {\n              color: '#61B9C8',\n              showMaxLabel: false,\n              fontSize: 10\n            },\n            splitLine: {\n              show: false,\n            },\n            name: '(次)',\n            nameGap: -5,\n            nameTextStyle: {\n              color: '#61B9C8',\n              fontSize: 9,\n              align: 'right',\n              padding: [0, 4, 0, 0]\n            },\n          }\n        ],\n        series: [\n          {\n            name: '消费次数',\n            type: 'bar',\n            itemStyle: {\n              color:{\n                type: 'linear',\n                x: 0,\n                y: 0,\n                x2: 0,\n                y2: 1,\n                colorStops: [{\n                  offset: 0, color: '#50A2F6' // 0% 处的颜色\n                },\n                  {\n                    offset: 0.4, color: '#9490F9' // 40% 处的颜色\n                  }, {\n                    offset: 1, color: '#DF7DFD' // 100% 处的颜色\n                  }],\n                global: false // 缺省为 false\n              } //背景渐变色\n            },\n            barWidth: 10,\n            barCategoryGap: 10,\n            data: []\n          },\n        ]\n      }\n    }\n  },\n  methods: {\n    setChart() {\n      let myChart = this.$echarts(this.$el);\n      myChart.clear();\n      myChart.resize()\n      myChart.setOption(this.option);\n    },\n    load() {\n      request.get(\"/DistanceConsume\").then(res => {\n        const { data } = res;\n        this.option.xAxis[0].data = data[0]\n        this.option.series[0].data = data[1]\n        this.setChart()\n      })\n    }\n  },\n  mounted() {\n    this.load()\n  },\n}\n</script>\n\n<style lang=\"less\" scoped>\n.DistanceConsume {\n    width: 100%;\n    height: 100%;\n}\n</style>"]}]}