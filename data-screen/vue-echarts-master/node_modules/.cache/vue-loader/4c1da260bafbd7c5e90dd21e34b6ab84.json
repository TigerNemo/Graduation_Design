>\n\n<script>\nexport default {\n    name: '',\n    data() {\n        return {}\n    },\n    methods: {\n        setChart() {\n            let option = {\n                tooltip: {\n                    trigger: 'axis',\n                    axisPointer: {            // 坐标轴指示器，坐标轴触发有效\n                        type: 'shadow'        // 默认为直线，可选为：'line' | 'shadow'\n                    },\n                    backgroundColor: '#11367a',\n                    textStyle: {\n                        color: '#6dd0e3',\n                        fontSize: 10,\n                    },\n                },\n                legend: {\n                    left: \"11%\",\n                    top: \"15%\",\n                    itemWidth: 7,\n                    itemHeight: 7,\n                    textStyle: {\n                        color: '#5CB1C1',\n                    }\n                },\n                grid: {\n                    top: '12%',\n                    bottom: '12%',\n                    left: \"10%\",\n                    right: '10%',\n                    containLabel: false\n                },\n                xAxis: [\n                    {\n                        type: 'category',\n                        boundaryGap: true,\n                        axisLine: {\n                            symbol: ['none', 'arrow'],\n                            symbolSize: [6, 6],\n                            symbolOffset: [0, 10],\n                            lineStyle: {\n                                color: '#122C49'\n                            }\n                        },\n                        axisTick: {show: false},\n                        axisLabel: {\n                            color: '#61B9C8',\n                            fontSize: 10,\n                            interval: 0,\n                        },\n                        data: [\"1部\", \"2部\", \"3部\", \"4部\", \"5部\", \"6部\", \"7部\", \"8部\", \"9部\", \"10部\"]\n                    }\n                ],\n   