mplate>\n    <div class=\"EverydayDate\"></div>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: \"EverydayDate\",\n  data() {\n    return {\n      option: {\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: {            // 坐标轴指示器，坐标轴触发有效\n            type: 'line'        // 默认为直线，可选为：'line' | 'shadow'\n          },\n          backgroundColor: '#11367a',\n          textStyle: {\n            color: '#6dd0e3',\n            fontSize: 10,\n          },\n        },\n        grid: {\n          top: '12%',\n          bottom: '12%',\n          left: '9%',\n          right: '5%',\n        },\n        legend: {\n          right: '5%',\n          top: '10%',\n          itemWidth: 7,\n          itemHeight: 7,\n          textStyle: {\n            color: '#5CB1C1',\n          }\n        },\n        xAxis: {\n          type: 'category',\n          boundaryGap: false,\n          axisLabel: {\n            color: '#61B9C8',\n            fontSize: 10,\n          },\n          axisLine: {\n            symbol: ['none', 'arrow'],\n            symbolSize: [6, 6],\n            symbolOffset: [0, 5],\n            lineStyle: {\n              color: '#122C49'\n            }\n          },\n          axisTick: {\n            color: '#122C49',\n            inside: true\n          },\n          z: 2,\n          data: []\n        },\n        yAxis:\n            {\n              type: 'value',\n              interval: 200,\n              min: 0,\n              max: 1400+200,\n              splitNumber: 7,\n              axisLine: {\n                symbol: ['none', 'arrow'],\n                symbolSize: [6, 6],\n                lineStyle: {\n                  color: '#122C49'\n                }\n              },\n              axisLabel: {\n                color: '#61B9C8',\n                showMaxLabel: false,\n                fontSize: 10\n              },\n              splitLine: {\n                show: false,\n              },\n              name: '(个)',\n              nameGap: -10,\n              nameTextStyle: {\n                color: '#61B9C8',\n                fontSize: 9,\n                align: 'right',\n                padding: [0, 6, 0, 0]\n              },\n            },\n        series: [{\n          name: '核销个数',\n          type: 'line',\n          symbol: 'none',\n          smooth: true,\n          data: [],\n          lineStyle: {\n            width: 0\n          },\n          itemStyle: {\n            color: '#9e70ff'\n          },\n          areaStyle: {\n            color: {\n              type: 'linear',\n              x: 0,\n              y: 0,\n              x2: 0,\n              y2: 1,\n              colorStops: [{\n                offset: 0, color: '#9e70ff' // 0% 处的颜色\n              },\n                {\n                  offset: 0.4, color: '#9e70ff' // 40% 处的颜色\n                }, {\n                  offset: 1, color: '#6e5eff' // 100% 处的颜色\n                }],\n              global: false // 缺省为 false\n            }, //背景渐变色\n            origin: 'start'\n          }\n        },\n        ]\n      }\n    }\n  },\n  methods: {\n    setChart() {\n      let myChart = this.$echarts(this.$el);\n      myChart.clear();\n      myChart.resize()\n      myChart.setOption(this.option);\n    },\n    load() {\n      request.get(\"/EverydayDate\").then(res => {\n        const { data } = res;\n        this.option.xAxis.data = data[0]\n        this.option.series[0].data = data[1]\n        this.setChart()\n      })\n    }\n  },\n  mounted() {\n    this.load()\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.EverydayDate {\n    width: 100%;\n    height: 100%;\n}\n</style>"]}]}